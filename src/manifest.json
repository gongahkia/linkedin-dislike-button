{
  "manifest_version": 3,
  "name": "LinkedIn Dislike Button",
  "version": "1.0",
  "description": "Browser Extenstion that adds a Dislike button to LinkedIn.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://*.linkedin.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
